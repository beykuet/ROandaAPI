qa <- function(wa, wb, index){
mean_wa <- mean(wa[index])
mean_wb <- mean(wb[index])
lambda <- 1
risk_wa <- lambda*sd(wa[index])
risk_wb <- lambda*sd(wb[index])
# output depending optimization result
if(mean_wa-risk_wa > mean_wb-risk_wb){
mean(wa[-index])
} else{
mean(wb[-index])
}
}
